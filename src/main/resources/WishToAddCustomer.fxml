<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1" fx:controller="core.controller.BusinessBookingController">
   <children>
      <Pane prefHeight="150.0" prefWidth="384.0" style="-fx-background-image: url('http://i347.photobucket.com/albums/p471/phi_nguyen8/appointmentbookingsystemGUIbg_zpsmk6gscbu.png');">
         <children>
            <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#1f93ff00" height="150.0" stroke="WHITE" strokeType="INSIDE" strokeWidth="5.0" width="384.0" />
            <Label fx:id="lblRegisterStatus" alignment="CENTER" layoutX="26.0" prefHeight="94.0" prefWidth="333.0" text="Customer does not exist, do you wish to add the customer to the system?" textAlignment="CENTER" textFill="WHITE" wrapText="true">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Button fx:id="AddCust" layoutX="26.0" layoutY="94.0" mnemonicParsing="false" onAction="#btnCancel" prefHeight="40.0" prefWidth="132.0" style="-fx-background-color: #ffffff;" text="Cancel" textAlignment="CENTER" textFill="#1565c0">
               <font>
                  <Font size="16.0" />
               </font>
               <styleClass>
                  <String fx:value="btnStyle1Pressed" />
                  <String fx:value="btnStyle1RegularState" />
                  <String fx:value="btnStyle1Hover" />
               </styleClass></Button>
            <Button fx:id="LoginButton1" layoutX="227.0" layoutY="94.0" mnemonicParsing="false" onAction="#btnContinue" prefHeight="40.0" prefWidth="132.0" style="-fx-background-color: #ffffff;" text="Continue" textAlignment="CENTER" textFill="#1565c0">
               <font>
                  <Font size="16.0" />
               </font>
               <styleClass>
                  <String fx:value="btnStyle1Pressed" />
                  <String fx:value="btnStyle1RegularState" />
                  <String fx:value="btnStyle1Hover" />
               </styleClass>
            </Button>
         </children>
      </Pane>
   </children>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
